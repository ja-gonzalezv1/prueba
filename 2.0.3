def ratio_fibonacci (lista):
    n= len(lista)
    ratio= []
    for i in range (1, n-1):
        fn= lista[i]
        fnm= lista[i+1]
        q= fnm/fn
        ratio.append(q)
    return ratio

y1= ratio_fibonacci(y)
x1= np.linspace(1, len(y1), len(y1))
aureo= (1+(5)**(1/2))/2

plt.plot(x1,y1)
