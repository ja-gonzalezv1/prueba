import numpy as np
import matplotlib.pyplot as plt

def fibonacci(n):
    secuencia= [0,1]
    for i in range(n-2):
        fnm= secuencia[i]
        fn= secuencia[i+1]
        fnp= fnm + fn
        secuencia.append(fnp)
    return secuencia

y= fibonacci(20)
x= np.linspace(1,20,20)
